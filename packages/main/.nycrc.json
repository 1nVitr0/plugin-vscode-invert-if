{
  "extends": ["@istanbuljs/nyc-config-typescript"],
  "reporter": ["lcov"],
  "require": ["source-map-support/register", "ts-node/register"],
  "include": ["out/"],
  "exclude": ["out/test/**/*.test.js"],
  "all": true,
  "sourceMap": true,
  "produce-source-map": true,
  "instrument": true,
  "hookRequire": true,
  "hookRunInContext": true,
  "hookRunInThisContext": true,
  "branches": 85,
  "lines": 95,
  "statements": 95,
  "functions": 100
}
