{
  "name": "invert-if-js",
  "version": "1.0.0",
  "preview": true,
  "displayName": "Invert If JavaScript Support",
  "description": "JavaScript, TypeScript, JSX, Flow and Babylon support for Invert If extension",
  "author": "1nVitr0",
  "publisher": "1nVitr0",
  "repository": {
    "type": "git",
    "url": "https://github.com/1nVitr0/plugin-vscode-invert-if.git"
  },
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.60.0"
  },
  "extensionKind": [
    "workspace",
    "ui"
  ],
  "categories": [
    "Formatters",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "refactor",
    "invert if condition",
    "negate condition",
    "swap if else",
    "guard clause"
  ],
  "extensionDependencies": [
    "1nVitr0.invert-if"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:flow",
    "onLanguage:babylon",
    "onCommand:invertIf.loadPlugins"
  ],
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "ast-types": "^0.16.1",
    "babylon": "^6.18.0",
    "recast": "^0.23.9"
  }
}
